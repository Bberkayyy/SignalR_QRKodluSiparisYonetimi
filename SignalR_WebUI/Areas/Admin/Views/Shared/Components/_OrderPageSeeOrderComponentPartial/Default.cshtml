@model ResultOrderDetailDto
<table class="table mt-3">
    <thead>
        <tr>
            <th></th>
            <th scope="col">Ürün Adı</th>
            <th scope="col">Birim Fiyatı</th>
            <th scope="col">Adeti</th>
            <th scope="col">Sil</th>
            <th scope="col">Güncelle</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td></td>
            <td>@Model.productName</td>
            <td>@Model.unitPrice</td>
            <td class="text-center">@Model.productCount</td>
            <td><a href="/Admin/OrderDetail/DeleteOrderDetail/@Model.id" class="btn btn-outline-danger btn-round">Sil</a></td>
            <td><a href="/Admin/OrderDetail/UpdateOrderDetail/@Model.orderTableName/@Model.id" class="btn btn-outline-success btn-round">Güncelle</a></td>
        </tr>
    </tbody>
</table>
